{
  "name": "telegramme",
  "version": "0.3.0",
  "private": false,
  "unpkg": "dist/telegram-me.min.js",
  "browser": {
    "./sfc": "src/components/telegram-me.vue"
  },
  "main": "src/components/telegram-me.vue",
  "keywords": [
    "telegram",
    "vue",
    "send",
    "message",
    "chat",
    "bot",
    "telegramme"
  ],
  "licenses": [
    {
      "type": "LGPL-3.0",
      "url": "https://github.com/stanislavec/telegramme/blob/master/LICENSE"
    }
  ],
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanislavec/telegramme.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "create-npm": "npx vue-cli-service build --target wc --name telegram-me ./src/components/telegram-me.vue",
    "prettier": "prettier --config ./prettier.config.js --write \"./src/**/*.{js,vue}\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.10",
    "prettier": "^2.0.2",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
